<?xml version="1.0" encoding="utf-8"?>
<!--shape 画背景-->
<shape xmlns:android="http://schemas.android.com/apk/res/android">
    <!--solid 填充色-->
    <solid android:color="@color/white"/>
    <!-- stroke 边框-->
    <stroke
        android:width="1px"
        android:color="#EDEDEE"/>
    <!-- corners 圆角-->
    <corners android:radius="4dp"/>
</shape>
