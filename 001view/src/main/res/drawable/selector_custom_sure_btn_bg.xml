<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <corners android:radius="8dp" />
            <stroke android:color="@color/default_login_btn_press" />
            <solid android:color="@color/default_login_btn_press" />
        </shape>
    </item>

    <item android:state_enabled="false">
        <shape>
            <corners android:radius="8dp" />
            <stroke android:color="#D3E5FB" />
            <solid android:color="@color/default_login_btn_unEnable" />
        </shape>
    </item>

    <item>
        <shape>
            <corners android:radius="8dp" />
            <stroke android:color="@color/default_login_btn_press" />
            <solid android:color="@color/theme_color" />
        </shape>
    </item>
</selector>